{
  "multipart": [
    {   "when": { "ishub": true },
      "apply": { "model": "yuushya:block/tube_hub" }
    },
    {
      "when": { "axis": "x","ishub": false },
      "apply": {"model": "yuushya:block/tube_horizontal", "x": 90, "y": 90}
    },
    {
      "when": { "axis": "y","ishub": false },
      "apply": {"model": "yuushya:block/tube"}
    },
    {
      "when": { "axis": "z","ishub": false },
      "apply": {"model": "yuushya:block/tube_horizontal", "x": 90}
    },
    {
      "when": { "ishub": true },
      "apply": { "model": "yuushya:block/tube_hub" }
    },
    {   "when": { "north": true},
      "apply": { "model": "yuushya:block/tube_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "yuushya:block/tube_side", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "yuushya:block/tube_side", "y": 180 }
    },
    {   "when": { "west": true },
      "apply": { "model": "yuushya:block/tube_side", "y": 270 }
    },
    {   "when": { "up": true },
      "apply": { "model": "yuushya:block/tube_side", "x": 270 }
    },
    {   "when": { "down": true },
      "apply": { "model": "yuushya:block/tube_side", "x": 90 }
    }
  ]
}